<h1>Sign up is simple and free </h1>
<form role="form" class="signUp" ng-submit="addPerson()">
    <label for="email">Email:</label>
    <input ng-model="email" type="email" class="form-control" id="uName">
  </div>
    <label for="pwd">Password:</label>
    <input ng-model="password" type="password" class="form-control" id="pwd">
  </div>


  <div class="checkbox">
    <label><input type="checkbox"> Remember me</label>
  </div>
    <button type="submit" class="btn btn-default">Submit</button>
  </div>
</form>
